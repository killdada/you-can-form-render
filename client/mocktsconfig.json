{
  "compilerOptions": {
    "target": "commonjs",
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./mock-dist"
  },
  "include": ["mock/**/*"],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist",
    "typings",
    "**/__test__",
    "test",
    "docs",
    "tests",
    "src/**/*",
    "config/**/*",
    ".umirc.ts"
  ]
}
